{
    "compilerOptions": {
      "outDir": "./dist",
      "module": "esnext",
      "target": "esnext",
      "lib": ["esnext", "webworker"],
      "alwaysStrict": true,
      "strict": true,
      "preserveConstEnums": true,
      "moduleResolution": "node",
      "sourceMap": true,
      "esModuleInterop": true,
      "resolveJsonModule": true,
      "types": [
        "@cloudflare/workers-types"
      ],
    },
    "include": ["src"],
    "exclude": ["node_modules", "dist", "test"]
  }
